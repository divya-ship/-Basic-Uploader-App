<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <shape


        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval"
        >
        <stroke
            android:width="5dp"
            android:color="@android:color/holo_blue_dark"/>
        <padding
            android:top="30dp"
            android:right="30dp"
            android:bottom="30dp"
            android:left="30dp"/>
